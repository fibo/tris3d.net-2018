{
  "name": "tris3d.net",
  "version": "0.0.0",
  "private": true,
  "description": "Play tic tac toe in 3d",
  "main": "src/client.js",
  "scripts": {
    "browserify": "browserify $npm_package_main -t babelify -o public/bundle.js",
    "build": "npm run browserify; npm run sass",
    "lint": "standard",
    "sass": "node-sass --include-path node_modules src/sass/main.scss --output-style compressed --output ./public/",
    "start": "node src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/tris3d.net.git"
  },
  "keywords": [
    "tris3d",
    "multiplayer",
    "server",
    "3d",
    "game"
  ],
  "author": "Gianluca Casati (http://g14n.info)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/tris3d.net/issues"
  },
  "homepage": "https://tris3d.net",
  "config": {
    "port": 3000
  },
  "dependencies": {
    "express": "4.16.3",
    "internal-ip": "^3.0.1",
    "socket.io": "2.1.1"
  },
  "devDependencies": {
    "bulma": "^0.7.1",
    "node-sass": "^4.9.2",
    "pre-commit": "^1.2.2",
    "redux-thunk": "^2.3.0",
    "standard": "^11.0.1",
    "trunx": "^0.6.1",
    "zeroconf-redux": "^3.4.1"
  }
}
